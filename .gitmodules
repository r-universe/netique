[submodule ".registry"]
	path = .registry
	url = https://github.com/r-universe-org/cran-to-git
	shallow = true
	branch = HEAD
[submodule "buildr"]
	path = buildr
	url = https://github.com/netique/buildr
	shallow = true
	registered = true
[submodule "pkgdown"]
	path = pkgdown
	url = https://github.com/r-lib/pkgdown
	shallow = true
	registered = false
